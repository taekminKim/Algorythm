			int distance = 0;
			int a = 0;
			int sumA = 0;
			int type = 0;
			for(int i=0; i<command; i++){
				String str[] = br.readLine().split(" ");
				type = Integer.parseInt(str[0]);
				a = Integer.parseInt(str[1]);
				
				
			}
			
			
			
		}
	}
}
