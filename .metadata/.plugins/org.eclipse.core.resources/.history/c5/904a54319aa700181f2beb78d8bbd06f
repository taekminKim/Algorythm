package swea;

import java.util.Scanner;

public class q4615 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int T = sc.nextInt();
		
		for(int tc=1 ; tc <=T; tc++){
			int N = sc.nextInt();
			int M = sc.nextInt();
			int arr[][] = new int[N][N];
			arr[N/2-1][N/2-1] = arr[N/2 +1][N/2 +1] = 100;
			arr[N/2+1][N/2-1] = arr[N/2 -1][N/2 +1] = 99;
			
			
			
			
		}
	}
}
