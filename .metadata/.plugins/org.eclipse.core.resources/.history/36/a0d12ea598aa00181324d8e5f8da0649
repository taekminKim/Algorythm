package swea;

import java.util.Scanner;

public class q3282 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		
		for(int tc = 1; tc <= T; tc++){
			int n = sc.nextInt();
			int k = sc.nextInt();
			int v[] = new int[n+1];
			int c[] = new int[n+1];
			for(int i=0; i<n; i++){
				v[i] = sc.nextInt();
				c[i] = sc.nextInt();
			}
			
		}
		
	}
}
