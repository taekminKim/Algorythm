package swea;

import java.util.Scanner;

public class q3408 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int T = sc.nextInt();
		
		for(int tc = 1; tc<=T; tc++){
			int n = sc.nextInt();
			int s1 = (2+(n-1)/2)*n;
			int s2 = n*n;
			int s3 = n*n + n;
			
		}
	}
}
