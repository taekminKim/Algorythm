package swea;

import java.util.Scanner;

public class q3233 {
	public static void main(String[] args) {
		// 1 4 9 16 25
		//D[i] = D[i-1] + 2i + 1
		Scanner sc = new Scanner(System.in);
		
		int T  = sc.nextInt();
		
		for(int tc = 1; tc<=T; tc++){
			long a = sc.nextLong();
			long b = sc.nextLong();
			if(b!=1){
				a/=b;
			}
			System.out.println("#"+tc+" "+(a*a));
		}
	}
}
